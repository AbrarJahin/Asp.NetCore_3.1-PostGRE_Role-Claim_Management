{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.5.3",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.js.map",
        "css/bootstrap.css",
        "css/bootstrap.css.map"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/additional-methods.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@microsoft/signalr@3.1.10",
      "destination": "wwwroot/lib/microsoft-signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "css/bootstrap-datepicker.css",
        "css/bootstrap-datepicker.css.map",
        "js/bootstrap-datepicker.js"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.js",
        "js/dataTables.bootstrap4.js",
        "css/jquery.dataTables.css",
        "css/dataTables.bootstrap4.css",
        "images/sort_asc.png",
        "images/sort_asc_disabled.png",
        "images/sort_both.png",
        "images/sort_desc.png",
        "images/sort_desc_disabled.png"
      ]
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.css",
        "fonts/fontawesome-webfont.woff2"
      ]
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/",
      "files": [
        "qrcode.js"
      ]
    }
  ]
}